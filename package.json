{
  "name": "vite-template-react",
  "version": "1.3.0",
  "scripts": {
    "start": "vite --port 3000 --open",
    "dev": "vite --port 3000 --open",
    "build": "vite build && sentry:sourcemaps",
    "serve": "vite preview",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org steven-eubank --project javascript-react ./dist && sentry-cli sourcemaps upload --org steven-eubank --project javascript-react ./dist"
  },
  "dependencies": {
    "@sentry/cli": "^2.19.4",
    "@sentry/react": "^7.58.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.2.1"
  }
}
